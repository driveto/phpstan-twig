{
    "name": "driveto/phpstan-twig",
    "description": "Twig extensions for PHPStan",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Driveto\\PhpstanTwig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Driveto\\PhpstanTwig\\Tests\\": "tests/"
        }
    },
    "authors": [
        {"name": "Jan Matosik", "email": "jan.matosik@gmail.com"}
    ],
    "require": {
        "php": "^8.0",
        "phpstan/phpstan": "^1.8",
        "nikic/php-parser": "^4.14"
    },
    "require-dev": {
        "consistence-community/coding-standard": "^3.11",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.3",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^8.3",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/framework-bundle": "^5.0 || ^6.0",
        "twig/twig": "^3.4"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
